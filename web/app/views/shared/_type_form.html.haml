.row
  .large-3.columns
    = render 'shared/side_nav'
  .large9
    %h4 #{@title}
    = form_for type_form, :html => { :class => "form-horizontal" } do |f|
      = render(partial: 'shared/form_error', object: type_form)
      = f.label :name
      = f.text_field :name
      = f.label :class_name
      = f.text_field :class_name
      = f.label :options
      = f.text_field :options
      = f.label :description
      = f.text_field :description
      = f.submit
$("#<%=@element.name%>").html('<%=j render partial: "rooms/#{@element.room.name}/#{@element.name}", locals: {el: @element} %>');
$(".circle").each(function() {
  if ($(this).is(".v-small-12, .v-small-6")) {
    $(this).width($(this).height());
  }
  else {
    $(this).height($(this).width());
  }
});